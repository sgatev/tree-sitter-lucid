{
  "name": "tree-sitter-lucid",
  "version": "0.0.1",
  "description": "Lucid grammar for Tree-sitter",
  "repository": {
    "type": "git",
    "url": "https://github.com/sgatev/tree-sitter-lucid"
  },
  "main": "index.js",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "author": "Stanislav Gatev <sg@sgatev.com> (https://github.com/sgatev)",
  "license": "MIT",
  "devDependencies": {
    "tree-sitter-cli": "^0.22.2",
    "prebuildify": "^6.0.0"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "dependencies": {
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "tree-sitter": [
    {
      "scope": "source.lucid",
      "file-types": [
        "lu"
      ],
      "injection-regex": "lucid",
      "highlights": [
        "queries/highlights.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ]
    }
  ]
}
